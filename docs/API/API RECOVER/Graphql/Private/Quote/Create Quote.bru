meta {
  name: Create Quote
  type: graphql
  seq: 2
}

post {
  url: {{graphqlEndpoint}}
  body: graphql
  auth: bearer
}

auth:bearer {
  token: {{accessToken}}
}

body:graphql {
  mutation CreateQuote($userId: ID!) {
    createQuote(userId: $userId) {
      id
      scoringComplete
      offers {
          weeklyPrice
          leasingPeriod
          expiresAt
      }
    }
  }
  
}

body:graphql:vars {
  {
      "userId": "dd8b5b17-a4df-41a5-94e5-36460eb65f71" 
  }
}
