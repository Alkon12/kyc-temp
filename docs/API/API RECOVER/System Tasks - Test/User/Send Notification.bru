meta {
  name: Send Notification
  type: graphql
  seq: 1
}

post {
  url: {{graphqlEndpoint}}
  body: graphql
  auth: bearer
}

auth:bearer {
  token: {{accessToken}}
}

body:graphql {
  mutation sendNotification($userId: ID!, $input: SendNotificationInput!) {
    sendNotification(userId: $userId, input: $input) 
  }
  
}

body:graphql:vars {
  {
      "userId": "1a675c7e-a2a8-4bb2-8fde-31a33a75df87",
      "input": {
          "channel": "EMAIL"
      }
  }
}
