meta {
  name: Active Leasings
  type: graphql
  seq: 1
}

post {
  url: {{graphqlEndpoint}}
  body: graphql
  auth: bearer
}

auth:bearer {
  token: {{accessToken}}
}

body:graphql {
  query activeLeasings {
    activeLeasings {
      id        
      createdAt
      updatedAt
      startDate
      endDate
      vehicle {
          id     
          product {
              brand
              model
          }       
      }
      user {
          email
          fullName
      }
    }
  }
}
