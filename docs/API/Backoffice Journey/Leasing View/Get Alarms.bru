meta {
  name: Get Alarms
  type: graphql
  seq: 4
}

post {
  url: {{graphqlEndpoint}}
  body: graphql
  auth: bearer
}

auth:bearer {
  token: {{accessToken}}
}

body:graphql {
  query leasingAlarms($leasingId: ID!) {
    leasingAlarms(leasingId: $leasingId) {
      alarms {
  			id
  			alertLevel
  			category
  			date
  			deviceId
  			lastUpdate
  			status
  			subcategory
      }
    }
  }
}

body:graphql:vars {
  {
      "leasingId": "38559d41-0b59-4245-9b4c-70da6bb77cd0" 
  }
}
