meta {
  name: Active Leads
  type: graphql
  seq: 1
}

post {
  url: {{graphqlEndpoint}}
  body: graphql
  auth: bearer
}

auth:bearer {
  token: {{accessToken}}
}

body:graphql {
  query ActiveLeads {
    activeLeads{
      id
      email
      firstName
      lastName
      phoneNumber
      hasUberAccount
    }
  }
}
