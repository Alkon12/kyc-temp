meta {
  name: Get Users By Group
  type: graphql
  seq: 8
}

post {
  url: {{graphqlEndpoint}}
  body: graphql
  auth: bearer
}

auth:bearer {
  token: {{accessToken}}
}

body:graphql {
  query usersByGroup($groupId: String!) {
    usersByGroup(groupId: $groupId) {
      id
      email
      fullName
      groups {
        id
      }
    }
  }
}

body:graphql:vars {
  {
      "groupId": "BACKOFFICE"
  }
}
