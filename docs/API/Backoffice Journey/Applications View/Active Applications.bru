meta {
  name: Active Applications
  type: graphql
  seq: 1
}

post {
  url: {{graphqlEndpoint}}
  body: graphql
  auth: bearer
}

auth:bearer {
  token: {{accessToken}}
}

body:graphql {
  query ActiveApplications {
    activeApplications{
      id
      hasDriverEngaged
      hasKycFinished
      createdAt
      updatedAt
      user {
          email
          fullName
      }
      product {
          brand
          model
      }
    }
  }
}
