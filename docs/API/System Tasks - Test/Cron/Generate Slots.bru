meta {
  name: Generate Slots
  type: graphql
  seq: 2
}

post {
  url: {{graphqlEndpoint}}
  body: graphql
  auth: bearer
}

auth:bearer {
  token: {{accessToken}}
}

body:graphql {
  mutation populateSlots($slotType: String!, $dateFrom: DateTime!, $dateTo: DateTime!) {
    populateSlots(slotType: $slotType, dateFrom: $dateFrom, dateTo: $dateTo) {
      id
      startsAt
      endsAt
      hostUserId
    }
  }
}

body:graphql:vars {
  {
      "slotType":"KYC_MEET",
      "dateFrom": "2024-11-01",
      "dateTo": "2024-12-31"
  }
}
